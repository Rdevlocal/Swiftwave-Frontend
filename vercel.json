{
    "version": 2,
    "env": {
      "MY_VARIABLE": true
    },
    "build": {
      "env": {
        "MY_VARIABLE": true
      }
    },
    "builds": [
      {
        "src": "nuxt.config.js",
        "use": "@nuxtjs/vercel-builder",
        "config": {
          "serverFiles": ["server-middleware/**"]
        }
      }
      {
        "buildCommand": "next build"
      }
      {
        "devCommand": "next dev"
      }
    ],
    "routes": [
      {
        "src": "/app",
        "dest": "/app/page.tsx"
      }
    ]
  }