{
    "version": 2,
    "env": {
      "MY_VARIABLE": "true"  // Zorg ervoor dat dit een string is
    },
    "builds": [
      {
        "src": "nuxt.config.js",
        "use": "@nuxtjs/vercel-builder",
        "config": {
          "serverFiles": ["server-middleware/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/app",
        "dest": "/app/page.tsx"
      }
    ]
  }
  